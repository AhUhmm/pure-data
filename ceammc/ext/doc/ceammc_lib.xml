<?xml version="1.0" encoding="UTF-8"?>
<library xmlns:xi="http://www.w3.org/2001/XInclude" name="ceammc" version="1.0">
  <category name="dyn">
    <entry descr="stereo dynamic range compressor" name="dyn.comp2~">
      <xi:include href="dyn.comp2~.pddoc" parse="xml"/>
    </entry>
    <entry descr="mono dynamic range compressors" name="dyn.comp~">
      <xi:include href="dyn.comp~.pddoc" parse="xml"/>
    </entry>
    <entry descr="stereo signal gate" name="dyn.gate2~">
      <xi:include href="dyn.gate2~.pddoc" parse="xml"/>
    </entry>
    <entry descr="mono signal gate" name="dyn.gate~">
      <xi:include href="dyn.gate~.pddoc" parse="xml"/>
    </entry>
    <entry descr="stereo 1176LN Peak Limiter" name="dyn.limit2~">
      <xi:include href="dyn.limit2~.pddoc" parse="xml"/>
    </entry>
    <entry descr="mono 1176LN Peak Limiter" name="dyn.limit~">
      <xi:include href="dyn.limit~.pddoc" parse="xml"/>
    </entry>
  </category>
  <category name="env">
    <entry descr="ADSR (Attack, Decay, Sustain, Release) envelope generator" name="env.adsr~">
      <xi:include href="env.adsr~.pddoc" parse="xml"/>
    </entry>
    <entry descr="AR (Attack, Release) envelope generator (useful to create percussion envelopes)." name="env.ar~">
      <xi:include href="env.ar~.pddoc" parse="xml"/>
    </entry>
  </category>
  <category name="lfo">
    <entry descr="Unit-amplitude low-frequency impulse train" name="lfo.impulse~">
      <xi:include href="lfo.impulse~.pddoc" parse="xml"/>
    </entry>
  </category>
  <category name="list">
    <entry descr="modifies list value at specified position, filtering it via external object." name="list.apply_to">
      <xi:include href="list.apply_to.pddoc" parse="xml"/>
    </entry>
    <xi:include href="ceammc_category_list.xml" parse="xml"/>
  </category>
  <category name="osc">
    <entry descr="sine oscilator with feedback" name="osc.sinfb~">
      <xi:include href="osc.sinfb~.pddoc" parse="xml"/>
    </entry>
  </category>
  <category name="predicates">
    <entry descr="checks if input data has *any* type" name="is_any">
      <xi:include href="is_any.pddoc" parse="xml"/>
    </entry>
    <entry descr="checks if input data has *bang* type" name="is_bang">
      <xi:include href="is_bang.pddoc" parse="xml"/>
    </entry>
    <entry descr="checks if input number is even" name="is_even">
      <xi:include href="is_even.pddoc" parse="xml"/>
    </entry>
    <entry descr="checks if input data has *float* type" name="is_float">
      <xi:include href="is_float.pddoc" parse="xml"/>
    </entry>
    <entry descr="checks if input data has *list* type" name="is_list">
      <xi:include href="is_list.pddoc" parse="xml"/>
    </entry>
    <entry descr="checks if input data has *symbol* type" name="is_symbol">
      <xi:include href="is_symbol.pddoc" parse="xml"/>
    </entry>
  </category>
  <category name="system">
    <entry descr="returns the size of physical memory (RAM) in bytes round to float data type." name="system.memsize">
      <xi:include href="system.memsize.pddoc" parse="xml"/>
    </entry>
    <entry descr="returns the process physical memory use - current and peak" name="system.memused">
      <xi:include href="system.memused.pddoc" parse="xml"/>
    </entry>
  </category>
  <meta>
    <version>0.1</version>
    <authors>
      <author>Alex Nadzharov</author>
      <author>Serge Poltavsky</author>
    </authors>
    <xi:include href="ceammc_meta.xml" parse="xml"/>
  </meta>
</library>
