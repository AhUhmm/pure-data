<?xml version="1.0" encoding="UTF-8"?>
<pddoc version="1.0">
    <object name="flt.c_highshelf~">
        <title>flt.c_highshelf~</title>
        <meta>
            <authors>
                <author>Alex Nadzharov</author>
                <author>Serge Poltavsky</author>
            </authors>
            <description>Highshelf filter coefficent calculator for biquad</description>
            <license>GPL3</license>
            <library>ceammc</library>
            <category>flt</category>
            <keywords>filter highshelf</keywords>
            <since>0.1</since>
            <also>
                <see>flt.highshelf~</see>
                <see>flt.biquad~</see>
            </also>
        </meta>
        <info>
            <par>gain boost|cut above some frequency</par>
        </info>
        <methods>
            <method name="freq">
                <param type="float" units="herz">cutoff frequency</param>
            </method>
            <method name="q">
                <param type="float" name="Q" minvalue="0.01" maxvalue="100" default="0.01">quality factor</param>
            </method>
        </methods>
        <arguments>
            <argument name="freq" type="float" units="herz">cutoff frequency</argument>
            <argument name="q" type="float" minvalue="0.01" maxvalue="100">quality factory</argument>
            <argument name="ID" type="symbol">object ID for OSC control</argument>
        </arguments>
        <inlets>
            <inlet type="audio"><xinfo>input signal</xinfo></inlet>
        </inlets>
        <outlets>
            <outlet type="audio">b0</outlet>
            <outlet type="audio">b1</outlet>
            <outlet type="audio">b2</outlet>
            <outlet type="audio">a1</outlet>
            <outlet type="audio">a2</outlet>
        </outlets>
        <example>
        <pdascii>
<![CDATA[
[noise.pink~]
|
|  [T]
|  |.
[*~ ]
|
|    [F]       [F]
|    |         |
|    [freq $1( [gain $1(
|    |         |
|    [flt.c_highshelf~  1000 1 -15]
|    |  ^|   ^^|    ^^^|     ^^^^|
|    |.  |..   |...    |....     |.....
[flt.biquad~                            ]
|\
[dac~]

]]>
        </pdascii>
        </example>
    </object>
</pddoc>

